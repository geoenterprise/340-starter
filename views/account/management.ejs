<% if (title) { %>
<h1><%= title %></h1>
<% } %> <% if (messages.error) { %>
<p class="error"><%= messages.error %></p>
<% } %> <% if (messages.success) { %>
<p class="success"><%= messages.success %></p>
<% } %> <% if (messages.notice) { %>
<p class="notice"><%= messages.notice %></p>
<% } %>

<h2>Welcome <%= account_firstname %></h2>

<h3>You're logged in</h3>

<p>
  <a href="/account/update/<%= accountData.account_id %>"
    >Update Account Information</a
  >
</p>

<% if (accountData.account_type === 'Employee' || accountData.account_type ===
'Admin') { %>
<h3>Inventory Management</h3>
<p><a href="/inv/management">Manage Inventory</a></p>
<% } %> <% if (accountData.account_type === 'Client') { %>
<h3>Review Management</h3>
<p><a href="/reviews/management">My Reviews</a></p>
<% } %>
